{
  "extends": "airbnb/legacy",
  "env": {
    "amd": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-only-tests/no-only-tests": 2,
    // Exceptions
    "func-names": 0,
    "comma-dangle": ["error", "always-multiline"],
    // Temporary
    "no-underscore-dangle": 0
  },
  "plugins": ["no-only-tests"]
}
