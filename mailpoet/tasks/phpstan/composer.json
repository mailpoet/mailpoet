{
  "require": {
    "php-stubs/woocommerce-stubs": "8.3.1",
    "phpstan/phpstan": "1.10.46",
    "phpstan/phpstan-doctrine": "1.3.53",
    "phpstan/phpstan-phpunit": "1.3.15",
    "szepeviktor/phpstan-wordpress": "1.3.2"
  },
  "autoload": {
    "psr-4": {
      "MailPoet\\PHPStan\\Extensions\\": "extensions"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "php prefix-phpstan-doctrine.php"
    ],
    "post-update-cmd": [
      "php prefix-phpstan-doctrine.php"
    ]
  }
}
