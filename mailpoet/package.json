  "engines": {
    "npm": "^8.1"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "scripts": {
    "lint": "npm run lint6 && npm run lint-ts && npm run lint5 && npm run lint-tests",
    "lint6": "../eslint-config/node_modules/.bin/eslint -c ../eslint-config/.eslintrc.es6.json --max-warnings 0 'assets/js/src/**/*.jsx' 'tests/javascript/**/*.js'",
    "lint-ts": "../eslint-config/node_modules/.bin/eslint -c ../eslint-config/.eslintrc.ts.json --max-warnings 0 'assets/js/src/**/*.tsx' 'assets/js/src/**/*.ts'",
    "lint5": "../eslint-config/node_modules/.bin/eslint -c ../eslint-config/.eslintrc.es5.json --max-warnings 0 'assets/js/src/**/*.js' 'webpack.config.js'",
    "lint-tests": "../eslint-config/node_modules/.bin/eslint -c ../eslint-config/.eslintrc.tests_newsletter_editor.json --max-warnings 0 'tests/javascript_newsletter_editor'",
    "autoprefixer": "postcss assets/dist/css/*.css --use autoprefixer --no-map --replace",
    "scss": "sass assets/css/src/:assets/dist/css/ --style compressed",
    "stylelint": "stylelint --fix",
    "stylelint-check": "stylelint",
    "test": "env NODE_PATH=$NODE_PATH:./assets/js/src mocha --recursive --require tests/javascript/babel_register.js  tests/javascript --extension spec.js --extension spec.ts",
    "check-types": "tsc --noEmit",
    "storybook": "start-storybook -s ./ -p 8083",
    "build-storybook": "build-storybook",
    "postinstall": "./tasks/npm_post_install.sh",
    "prepare": "cd .. && husky install"
  },
  "lint-staged": {
    "*.{scss,css}": "npm run stylelint",
    "!(*spec).js": "../eslint-config/node_modules/.bin/eslint -c ../eslint-config/.eslintrc.es5.json --max-warnings 0",
    "*.jsx": "../eslint-config/node_modules/.bin/eslint -c ../eslint-config/.eslintrc.es6.json --max-warnings 0",
    "*.{tsx,ts}": "../eslint-config/node_modules/.bin/eslint -c ../eslint-config/.eslintrc.ts.json --max-warnings 0",
    "**/newsletter_editor/**/*spec.js": "../eslint-config/node_modules/.bin/eslint -c ../eslint-config/.eslintrc.tests_newsletter_editor.json --max-warnings 0",
    "*.php": [
      "phplint",
      "./do qa:code-sniffer",
      "bash -c './do qa:phpstan'"
    ]
  },
