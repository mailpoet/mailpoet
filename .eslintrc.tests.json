{
  "extends": "airbnb/legacy",
  "env": {
    "amd": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    // Exceptions
    "func-names": 0,
    // To add
    "no-bitwise": 0,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "padded-blocks": 0
  }
}
