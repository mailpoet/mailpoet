{
  "extends": "airbnb/legacy",
  "env": {
    "amd": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "import/no-amd": 0,
    "no-whitespace-before-property": 0,
    "global-require": 0,
    "keyword-spacing": 0,
    "no-bitwise": 0,
    "no-spaced-func": 0,
    "func-call-spacing": 0,
    "max-len": 0,
    "space-unary-ops": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "padded-blocks": 0,
    "space-before-blocks": 0,
    "object-curly-spacing": 0,
    "func-names": 0,
    "space-before-function-paren": 0
  }
}
